            <TextLine id="tr_1_tl_35" custom="readingOrder {index:34;}">
               <Coords points="591,3300 3030,3271 3030,3188 595,3248"/>
               <Baseline points="614,3289 735,3292 856,3291 977,3289 1098,3287 1220,3285 1341,3283 1462,3280 1583,3277 1704,3274 1826,3271 1947,3271 2068,3268 2189,3268 2310,3265 2432,3262 2553,3256 2674,3253 2795,3251 2916,3248 3038,3250"/>
               <TextEquiv>
                  <Unicode>verstehe ich – um ganz ehrlich zu sein – nicht genau, was Sie meinen. Genau das</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_36" custom="readingOrder {index:35;}">
               <Coords points="591,3367 2504,3383 3185,3340 3185,3254 591,3310"/>
               <Baseline points="611,3357 740,3362 869,3360 998,3357 1127,3354 1256,3351 1385,3349 1514,3348 1643,3344 1772,3342 1902,3342 2031,3339 2160,3333 2289,3333 2418,3330 2547,3329 2676,3325 2805,3322 2934,3319 3063,3313 3193,3313"/>
               <TextEquiv>
                  <Unicode>Gleiche gilt natürlich für die Schweiz, und für jeden Ecken des brennenden Erd¬</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_37" custom="readingOrder {index:36;}">
               <Coords points="588,3449 634,3443 661,3449 677,3466 690,3459 697,3466 707,3456 720,3466 753,3462 776,3439 790,3452 806,3452 806,3386 766,3386 750,3376 710,3386 697,3373 677,3393 651,3396 608,3363 588,3367"/>
               <Baseline points="605,3432 631,3432 657,3432 683,3434 709,3434 735,3432 761,3432 787,3432 814,3432"/>
               <TextEquiv>
                  <Unicode>balls.\:p\</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_38" custom="readingOrder {index:37;}">
               <Coords points="588,3522 3106,3489 3135,3409 2749,3347 2128,3443 1896,3376 1662,3449 1292,3396 588,3449"/>
               <Baseline points="605,3501 731,3504 858,3501 985,3501 1111,3498 1238,3493 1365,3492 1491,3489 1618,3489 1745,3486 1872,3482 1998,3481 2125,3477 2252,3476 2378,3471 2505,3471 2632,3465 2758,3465 2885,3460 3012,3459 3139,3453"/>
               <TextEquiv>
                  <Unicode>\p\Kommen wir zu einem praktischen Ende: wenn ich im Verlauf des Sommers irgend¬</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_39" custom="readingOrder {index:38;}">
               <Coords points="595,3588 2881,3568 3069,3555 3072,3485 595,3528"/>
               <Baseline points="614,3572 737,3573 860,3570 983,3569 1106,3566 1229,3564 1352,3563 1476,3560 1599,3560 1722,3557 1845,3553 1968,3551 2091,3548 2214,3546 2338,3544 2461,3542 2584,3539 2707,3534 2830,3532 2953,3528 3077,3528"/>
               <TextEquiv>
                  <Unicode>welche Aufträge für Sie machen kann, so bin ich dazu freudig bereit. Ab Ende</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_40" custom="readingOrder {index:39;}">
               <Coords points="595,3661 3205,3614 3205,3548 598,3591"/>
               <Baseline points="614,3647 743,3644 873,3641 1003,3641 1132,3638 1262,3635 1392,3632 1521,3632 1651,3626 1781,3626 1911,3623 2040,3620 2170,3617 2300,3614 2429,3611 2559,3610 2689,3605 2818,3602 2948,3602 3078,3599 3208,3599"/>
               <TextEquiv>
                  <Unicode>dieses Monats – denn bis dahin habe ich mit dem Saladin zu tun. Es wäre mir sogar</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_41" custom="readingOrder {index:40;}">
               <Coords points="591,3740 3079,3690 3003,3614 595,3657"/>
               <Baseline points="611,3716 734,3716 858,3716 982,3713 1106,3710 1229,3710 1353,3704 1477,3701 1601,3698 1724,3698 1848,3695 1972,3695 2096,3689 2219,3687 2343,3683 2467,3680 2591,3680 2714,3675 2838,3673 2962,3669 3086,3671"/>
               <TextEquiv>
                  <Unicode>besonders lieb, wenn ich \g\vor\:g\ einer neuen grösseren Unternehmung etwas mit</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_42" custom="readingOrder {index:41;}">
               <Coords points="591,3799 3132,3806 3135,3667 595,3737"/>
               <Baseline points="611,3784 737,3786 864,3784 991,3781 1117,3779 1244,3778 1371,3775 1497,3772 1624,3769 1751,3766 1878,3764 2004,3761 2131,3759 2258,3754 2384,3751 2511,3751 2638,3746 2764,3744 2891,3739 3018,3739 3145,3742"/>
               <TextEquiv>
                  <Unicode>Jhnen arbeiten und besprechen könnte, – um besser zu wissen, was und wie Sie es</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_43" custom="readingOrder {index:42;}">
               <Coords points="588,3869 648,3865 687,3892 753,3895 816,3855 859,3855 889,3879 948,3862 1074,3872 1074,3819 994,3793 819,3816 737,3796 591,3796"/>
               <Baseline points="611,3859 637,3859 663,3857 689,3856 715,3856 742,3856 768,3856 794,3853 820,3850 847,3850 873,3850 899,3850 925,3851 951,3853 978,3853 1004,3853 1030,3853 1056,3851 1083,3847"/>
               <TextEquiv>
                  <Unicode>haben wollen. –\:p\</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_44" custom="readingOrder {index:43;}">
               <Coords points="595,3951 1873,3968 2940,3902 2940,3822 598,3872"/>
               <Baseline points="614,3921 730,3927 847,3924 963,3924 1080,3921 1196,3919 1313,3919 1429,3916 1546,3913 1662,3908 1779,3907 1896,3905 2012,3904 2129,3899 2245,3897 2362,3895 2478,3892 2595,3889 2711,3887 2828,3886 2945,3883"/>
               <TextEquiv>
                  <Unicode>\p\Wann fahren Sie ins Wallis? Für wie lange? – Grüssen Sie bitte Jhre Frau –\:p\</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_45" custom="readingOrder {index:44;}">
               <Coords points="1867,4074 1969,4054 2042,4067 2243,4060 2247,3955 2025,3984 1939,3955 1919,3974 1870,3981"/>
               <Baseline points="1889,4041 1915,4044 1941,4044 1967,4040 1993,4038 2019,4038 2045,4037 2071,4035 2097,4035 2123,4035 2149,4033 2175,4032 2201,4032 2227,4035 2253,4038"/>
               <TextEquiv>
                  <Unicode>Stets Ihre [Unterschrift handschriftlich hinzugefügt]</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_46" custom="readingOrder {index:45;}">
               <Coords points="2121,4192 2184,4202 2247,4169 2319,4206 2709,4150 2818,4212 2911,4199 2914,4064 2788,4040 2702,4077 2610,4067 2534,4116 2461,4093 2296,4116 2207,4060 2124,4064"/>
               <Baseline points="2143,4157 2181,4154 2220,4153 2259,4150 2298,4148 2337,4145 2375,4145 2414,4145 2453,4145 2492,4145 2531,4146 2569,4149 2608,4151 2647,4148 2686,4146 2725,4143 2763,4142 2802,4142 2841,4141 2880,4139 2919,4142"/>
               <TextEquiv>
                  <Unicode>Annemarie Clark</Unicode>
               </TextEquiv>
            </TextLine>
         </TextRegion>
