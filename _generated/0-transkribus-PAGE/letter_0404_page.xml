               <TextEquiv>
                  <Unicode>New York City, 16. Dez. 40</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_3" custom="readingOrder {index:1;}">
               <Coords points="1536,433 1820,436 1864,400 1999,441 2247,386 2440,461 2655,452 2631,306 1536,309"/>
               <Baseline points="1551,378 1606,376 1662,375 1717,375 1773,376 1828,376 1884,376 1939,377 1995,378 2050,378 2106,378 2161,378 2217,378 2272,381 2328,378 2383,380 2439,381 2494,378 2550,381 2605,381 2661,384"/>
               <TextEquiv>
                  <Unicode>The Bedford Hotel, 118 East 40 St.</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_4" custom="readingOrder {index:2;}">
               <Coords points="1125,987 1197,995 1271,957 1379,987 1406,968 1453,984 1475,960 1497,982 1550,965 1547,852 1464,844 1395,863 1348,830 1282,863 1200,858 1122,811"/>
               <Baseline points="1139,901 1164,901 1190,901 1216,901 1242,901 1268,901 1294,901 1320,901 1346,901 1372,902 1398,903 1424,904 1450,904 1476,906 1502,907 1528,909 1554,913"/>
               <TextEquiv>
                  <Unicode>Mein Mameli,</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_5" custom="readingOrder {index:3;}">
               <Coords points="915,1106 3039,1125 3014,1042 2650,954 2032,1023 1412,968 1310,1031 915,957"/>
               <Baseline points="931,1074 1036,1074 1142,1076 1248,1077 1354,1078 1459,1080 1565,1080 1671,1080 1777,1080 1882,1083 1988,1083 2094,1086 2200,1086 2305,1083 2411,1083 2517,1083 2623,1083 2728,1083 2834,1084 2940,1086 3046,1080"/>
               <TextEquiv>
                  <Unicode>\p\Dir schreiben, das ist das Einzige, was ich in diesen Weihnachts¬</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_6" custom="readingOrder {index:4;}">
               <Coords points="871,1222 2986,1208 2937,1139 2498,1089 871,1125"/>
               <Baseline points="886,1181 991,1179 1096,1180 1202,1181 1307,1181 1412,1181 1518,1181 1623,1184 1728,1183 1834,1184 1939,1186 2044,1184 2150,1185 2255,1187 2360,1187 2466,1187 2571,1187 2676,1187 2782,1188 2887,1190 2993,1196"/>
               <TextEquiv>
                  <Unicode>tagen tun kann, um Dir meine Liebe und Zärtlichkeit zu schicken.</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_7" custom="readingOrder {index:5;}">
               <Coords points="863,1293 3003,1332 3003,1252 2882,1213 863,1233"/>
               <Baseline points="877,1283 983,1283 1090,1285 1197,1286 1303,1286 1410,1289 1517,1286 1623,1289 1730,1292 1837,1292 1944,1295 2050,1292 2157,1292 2264,1292 2370,1292 2477,1292 2584,1295 2690,1295 2797,1295 2904,1296 3011,1304"/>
               <TextEquiv>
                  <Unicode>Wir feiern kein Fest diesmal, – und ich versuche, mir vorzustellen,</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_8" custom="readingOrder {index:6;}">
               <Coords points="858,1417 2887,1423 2887,1362 2755,1315 858,1302"/>
               <Baseline points="874,1387 975,1391 1076,1390 1177,1390 1278,1390 1379,1393 1480,1393 1581,1393 1682,1396 1783,1396 1884,1396 1985,1396 2086,1396 2187,1396 2288,1399 2389,1399 2490,1399 2591,1399 2692,1399 2793,1402 2894,1405"/>
               <TextEquiv>
                  <Unicode>wie es für Dich sein wird, mit Hasi, Adrienne, mit Meme, und dem</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_9" custom="readingOrder {index:7;}">
               <Coords points="858,1500 1106,1550 2956,1541 2956,1404 1734,1448 1459,1404 1070,1461 858,1417"/>
               <Baseline points="874,1495 978,1495 1082,1496 1187,1498 1291,1498 1396,1498 1500,1498 1605,1501 1709,1501 1814,1504 1918,1502 2022,1504 2127,1504 2231,1504 2336,1504 2440,1504 2545,1505 2649,1507 2754,1504 2858,1507 2963,1510"/>
               <TextEquiv>
                  <Unicode>kleinen Enkel, und ganz Bocken, zum ersten Mal im Esszimmer beim</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_10" custom="readingOrder {index:8;}">
               <Coords points="858,1635 3000,1652 2997,1530 855,1533"/>
               <Baseline points="871,1599 977,1599 1084,1599 1191,1599 1297,1601 1404,1602 1511,1604 1617,1604 1724,1605 1831,1608 1938,1608 2044,1608 2151,1608 2258,1608 2364,1608 2471,1610 2578,1608 2684,1611 2791,1611 2898,1614 3005,1623"/>
               <TextEquiv>
                  <Unicode>Baum versammelt ohne Paps. Ja, Du wirst nicht allein sein, Mameli, –</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_11" custom="readingOrder {index:9;}">
               <Coords points="866,1743 2413,1718 2774,1795 3077,1734 3077,1616 2860,1677 2694,1619 1613,1605 866,1657"/>
               <Baseline points="880,1706 990,1706 1100,1706 1210,1706 1321,1709 1431,1709 1541,1710 1651,1712 1762,1712 1872,1715 1982,1714 2092,1715 2203,1715 2313,1712 2423,1715 2533,1715 2644,1715 2754,1718 2864,1718 2974,1721 3085,1721"/>
               <TextEquiv>
                  <Unicode>aber alles wird auf Dir ruhen, und alle werden auf Dich schauen, und</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_12" custom="readingOrder {index:10;}">
               <Coords points="855,1814 990,1859 1100,1825 1293,1856 1332,1828 1437,1848 1517,1834 1514,1734 1368,1751 1299,1729 1257,1762 1012,1715 984,1743 915,1726 852,1751"/>
               <Baseline points="871,1811 903,1811 936,1811 968,1811 1001,1811 1034,1811 1066,1812 1099,1814 1132,1811 1164,1811 1197,1811 1230,1811 1262,1813 1295,1814 1328,1814 1360,1814 1393,1814 1426,1814 1458,1814 1491,1817 1524,1820"/>
               <TextEquiv>
                  <Unicode>auf Dich vertrauen.\:p\</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_13" custom="readingOrder {index:11;}">
               <Coords points="855,1944 3039,1961 3039,1864 2818,1845 855,1812"/>
               <Baseline points="871,1912 979,1916 1088,1918 1197,1918 1306,1920 1414,1921 1523,1923 1632,1921 1741,1921 1849,1924 1958,1927 2067,1927 2176,1930 2284,1927 2393,1927 2502,1927 2611,1924 2719,1927 2828,1927 2937,1927 3046,1930"/>
               <TextEquiv>
                  <Unicode>\p\Diese letzten Wochen sind vorbei gegangen ohne jede Wirklichkeit.</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_14" custom="readingOrder {index:12;}">
               <Coords points="866,2068 2912,2071 2986,2052 2984,1958 863,1958"/>
               <Baseline points="877,2023 982,2023 1088,2023 1193,2020 1299,2023 1405,2025 1510,2026 1616,2026 1722,2026 1827,2029 1933,2031 2039,2029 2144,2032 2250,2032 2356,2032 2461,2032 2567,2032 2673,2032 2778,2035 2884,2035 2990,2041"/>
               <TextEquiv>
                  <Unicode>Ich habe versucht, mich daran zu gewöhnen, und mich zu erholen. In</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_15" custom="readingOrder {index:13;}">
               <Coords points="852,2151 2432,2143 2694,2192 2917,2154 2917,2060 852,2074"/>
               <Baseline points="868,2124 970,2127 1073,2127 1175,2130 1278,2130 1381,2130 1483,2132 1586,2133 1689,2133 1791,2135 1894,2133 1997,2136 2099,2136 2202,2136 2305,2139 2407,2136 2510,2136 2613,2139 2715,2140 2818,2142 2921,2145"/>
               <TextEquiv>
                  <Unicode>New York mit seiner ewig lastenden Dämmerung ist es mir nicht</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_16" custom="readingOrder {index:14;}">
               <Coords points="866,2275 3003,2269 3000,2184 2371,2140 863,2143"/>
               <Baseline points="877,2235 983,2235 1090,2235 1197,2235 1303,2235 1410,2235 1517,2238 1623,2238 1730,2238 1837,2238 1944,2238 2050,2241 2157,2241 2264,2241 2370,2241 2477,2243 2584,2244 2690,2244 2797,2247 2904,2247 3011,2256"/>
               <TextEquiv>
                  <Unicode>gelungen. Jetzt bin ich in Greenwich, in einer Art von Sanatorium,</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_17" custom="readingOrder {index:15;}">
               <Coords points="852,2358 3030,2371 2964,2291 2559,2253 852,2272"/>
               <Baseline points="868,2339 976,2339 1084,2341 1193,2342 1301,2342 1410,2342 1518,2342 1627,2345 1735,2343 1844,2345 1952,2348 2060,2348 2169,2348 2277,2348 2386,2348 2494,2348 2603,2348 2711,2349 2820,2351 2928,2351 3037,2354"/>
               <TextEquiv>
                  <Unicode>und werde noch 8-14 Tage bleiben, bis ich mich besser und sicherer</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_18" custom="readingOrder {index:16;}">
               <Coords points="855,2454 3003,2496 3003,2385 2837,2363 1277,2355 1194,2410 855,2355"/>
               <Baseline points="871,2444 977,2444 1084,2444 1191,2447 1298,2447 1405,2447 1512,2449 1618,2450 1725,2450 1832,2450 1939,2453 2046,2450 2153,2452 2260,2453 2366,2454 2473,2455 2580,2453 2687,2453 2794,2455 2901,2455 3008,2455"/>
               <TextEquiv>
                  <Unicode>fühle. Dann gehe ich vielleicht nach Princeton, und muss mir einen</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_19" custom="readingOrder {index:17;}">
               <Coords points="841,2595 2967,2578 2967,2507 2520,2457 841,2468"/>
               <Baseline points="856,2551 967,2551 1079,2551 1190,2551 1302,2554 1413,2553 1525,2554 1636,2555 1748,2557 1859,2555 1971,2557 2082,2560 2194,2560 2305,2560 2417,2560 2528,2560 2640,2560 2751,2561 2863,2563 2975,2563"/>
               <TextEquiv>
                  <Unicode>Ruck geben, um wieder in die Arbeit hinein zu kommen. Es ist sehr</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_20" custom="readingOrder {index:18;}">
               <Coords points="855,2678 2749,2711 2970,2686 2970,2581 951,2567 855,2620"/>
               <Baseline points="871,2655 976,2655 1081,2657 1187,2658 1292,2658 1397,2658 1503,2658 1608,2658 1713,2658 1819,2662 1924,2664 2029,2664 2135,2664 2240,2664 2345,2664 2451,2664 2556,2664 2661,2664 2767,2667 2872,2667 2978,2670"/>
               <TextEquiv>
                  <Unicode>schwer für mich geworden. Es ist, als hätte ich keinen Boden mehr</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_21" custom="readingOrder {index:19;}">
               <Coords points="852,2777 1291,2829 2341,2780 2628,2843 3008,2799 3008,2711 2716,2689 852,2702"/>
               <Baseline points="868,2763 975,2763 1082,2763 1189,2763 1297,2762 1404,2763 1511,2766 1619,2766 1726,2766 1833,2768 1941,2769 2048,2769 2155,2769 2262,2769 2370,2770 2477,2772 2584,2772 2692,2774 2799,2775 2906,2775 3014,2778"/>
               <TextEquiv>
                  <Unicode>unter meinen Füssen. Papa's Tod hat mir meine Lage erst recht zum</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_22" custom="readingOrder {index:20;}">
               <Coords points="849,2912 1114,2901 1169,2931 1293,2895 1379,2926 1445,2882 1517,2906 1566,2882 1566,2815 1484,2785 1453,2815 1354,2807 1321,2832 1177,2799 1120,2824 1075,2791 1023,2824 938,2780 847,2782"/>
               <Baseline points="862,2864 897,2864 933,2864 968,2864 1004,2865 1039,2867 1075,2867 1110,2867 1146,2867 1181,2867 1217,2867 1252,2867 1288,2869 1323,2869 1359,2867 1394,2870 1430,2870 1465,2870 1501,2871 1536,2873 1572,2876"/>
               <TextEquiv>
                  <Unicode>Bewusstsein gebracht.\:p\</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_23" custom="readingOrder {index:21;}">
               <Coords points="849,3008 2942,3022 2920,2931 2457,2895 849,2917"/>
               <Baseline points="865,2972 969,2972 1073,2972 1177,2972 1281,2972 1385,2974 1489,2978 1594,2978 1698,2978 1802,2978 1906,2978 2010,2978 2114,2978 2218,2980 2323,2981 2427,2981 2531,2981 2635,2981 2739,2984 2843,2984 2948,2987"/>
               <TextEquiv>
                  <Unicode>\p\Aber ich will Dir jetzt nicht viel schreiben, liebes Mameli. Ich</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_24" custom="readingOrder {index:22;}">
               <Coords points="852,3099 2151,3091 2338,3141 2934,3094 2934,3019 852,3006"/>
               <Baseline points="868,3073 971,3076 1075,3076 1179,3076 1282,3076 1386,3078 1490,3079 1593,3079 1697,3079 1801,3082 1905,3085 2008,3082 2112,3082 2216,3088 2319,3085 2423,3085 2527,3088 2630,3086 2734,3087 2838,3088 2942,3088"/>
               <TextEquiv>
                  <Unicode>weiss, dass es mir bald besser gehen wird, und dann ist man auch</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_25" custom="readingOrder {index:23;}">
               <Coords points="847,3212 3036,3221 3036,3105 1872,3152 847,3102"/>
               <Baseline points="862,3181 970,3181 1079,3184 1188,3184 1297,3184 1406,3184 1515,3184 1624,3184 1733,3185 1842,3187 1951,3187 2059,3190 2168,3190 2277,3190 2386,3190 2495,3191 2604,3193 2713,3193 2822,3193 2931,3194 3040,3196"/>
               <TextEquiv>
                  <Unicode>wieder mutiger. Und Jeder wählt doch bis zu einem hohen Grade sein</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_26" custom="readingOrder {index:24;}">
               <Coords points="852,3328 1009,3295 1125,3320 1194,3292 1434,3359 1431,3207 1359,3251 1249,3248 1183,3215 1106,3248 946,3196 849,3212"/>
               <Baseline points="865,3285 893,3285 922,3285 950,3285 979,3285 1008,3285 1036,3285 1065,3285 1094,3285 1122,3287 1151,3288 1180,3288 1208,3288 1237,3288 1266,3288 1294,3288 1323,3288 1352,3288 1380,3290 1409,3291 1438,3291"/>
               <TextEquiv>
                  <Unicode>Schicksal selbst.\:p\</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_27" custom="readingOrder {index:25;}">
               <Coords points="841,3430 2967,3419 2931,3334 2529,3356 2079,3273 1550,3361 841,3326"/>
               <Baseline points="856,3393 967,3393 1079,3393 1190,3393 1302,3396 1413,3396 1525,3396 1636,3397 1748,3398 1859,3396 1971,3398 2082,3398 2194,3401 2305,3401 2417,3401 2528,3401 2640,3401 2751,3402 2863,3404 2975,3407"/>
               <TextEquiv>
                  <Unicode>\p\Ich bin übrigens ziemlich nahe von Freddy, der mich Samstag sehr</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_28" custom="readingOrder {index:26;}">
               <Coords points="844,3499 1233,3568 1431,3505 3025,3546 3025,3441 2589,3469 2451,3408 2112,3469 2035,3417 1941,3466 1751,3408 844,3428"/>
               <Baseline points="859,3497 967,3497 1076,3497 1184,3496 1293,3497 1402,3497 1510,3497 1619,3500 1727,3501 1836,3503 1945,3503 2053,3503 2162,3503 2270,3503 2379,3506 2488,3506 2596,3506 2705,3506 2813,3506 2922,3509 3031,3518"/>
               <TextEquiv>
                  <Unicode>lieb hier besuchte. Frau v. Opel kam heute noch, bevor sie abreiste,</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_29" custom="readingOrder {index:27;}">
               <Coords points="847,3651 2165,3618 3028,3684 3006,3543 847,3546"/>
               <Baseline points="862,3604 970,3601 1078,3601 1187,3601 1295,3603 1404,3604 1512,3604 1621,3607 1729,3608 1838,3610 1946,3610 2054,3609 2163,3610 2271,3610 2380,3613 2488,3614 2597,3613 2705,3613 2814,3613 2922,3616 3031,3625"/>
               <TextEquiv>
                  <Unicode>um ihren Mann im Süden zu treffen. Sie hätte Dir gern geschrieben,</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_30" custom="readingOrder {index:28;}">
               <Coords points="838,3720 1475,3781 1828,3720 3003,3761 3000,3679 2802,3645 836,3662"/>
               <Baseline points="853,3706 960,3706 1068,3709 1176,3708 1284,3708 1392,3709 1500,3709 1608,3709 1716,3712 1824,3712 1932,3712 2039,3715 2147,3715 2255,3715 2363,3717 2471,3718 2579,3721 2687,3718 2795,3721 2903,3723 3011,3721"/>
               <TextEquiv>
                  <Unicode>aber meinte dann, vielleicht sei es Dir lieber nicht, da Du sie ja</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_31" custom="readingOrder {index:29;}">
               <Coords points="836,3847 904,3830 1001,3847 1034,3827 1070,3858 1213,3866 1260,3844 1260,3717 1139,3758 1048,3742 1026,3764 998,3747 949,3761 866,3714 836,3720"/>
               <Baseline points="850,3813 876,3811 902,3810 928,3810 954,3811 980,3813 1006,3812 1032,3810 1059,3812 1085,3813 1111,3813 1137,3813 1163,3813 1189,3813 1215,3813 1241,3815 1268,3819"/>
               <TextEquiv>
                  <Unicode>kaum kennst.\:p\</Unicode>
               </TextEquiv>
            </TextLine>
