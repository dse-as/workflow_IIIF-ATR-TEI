            <TextLine id="l_30" custom="readingOrder {index:29;}">
               <Coords points="216,1485 1543,1485 1543,1428 216,1431"/>
               <Baseline points="226,1474 292,1476 358,1475 424,1474 490,1476 556,1476 622,1478 688,1478 754,1477 820,1475 887,1476 953,1478 1019,1476 1085,1476 1151,1478 1217,1476 1283,1476 1349,1476 1415,1476 1481,1476 1548,1476"/>
