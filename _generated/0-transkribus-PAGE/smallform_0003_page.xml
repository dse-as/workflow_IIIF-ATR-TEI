               <TextEquiv>
                  <Unicode>Geselle sein, wenn Ihr es begehrt!"\:p\</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="l_10" custom="readingOrder {index:9;}">
               <Coords points="210,595 1545,598 1528,538 210,563"/>
