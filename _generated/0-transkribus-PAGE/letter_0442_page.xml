               <Baseline points="629,2752 679,2752 729,2753 779,2756 829,2758 880,2758 930,2758 980,2758 1030,2758 1080,2758 1131,2758 1181,2759 1231,2762 1281,2764 1331,2764 1382,2764 1432,2764 1482,2764 1532,2764 1582,2765 1633,2758"/>
               <TextEquiv>
                  <Unicode>wohl in klaren Momenten. –\:p\</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_21" custom="readingOrder {index:20;}">
