               <TextEquiv>
                  <Unicode>\p\Wir sind tief eingeschneit! ganz wunderbar schön! Wenn Du weisst</Unicode>
               </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_22" custom="readingOrder {index:21;}">
               <Coords points="603,2977 3115,3041 3091,2903 2889,2963 2424,2889 603,2903"/>
               <Baseline points="621,2964 745,2967 870,2970 995,2972 1120,2970 1245,2976 1370,2978 1495,2979 1620,2980 1745,2982 1870,2982 1994,2987 2119,2988 2244,2988 2369,2991 2494,2991 2619,2992 2744,2995 2869,2997 2994,3000 3119,3000"/>
               <TextEquiv>
                  <Unicode>wann Du in Dein gemietetes Haus umziehst, berichte es mir – denn</Unicode>
               </TextEquiv>
            </TextLine>
